{
    "manifest_version": 2,
    "name": "musify",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["./lib/p5.min.js","./lib/p5.dom.min.js","./lib/p5.sound.min.js","index.js"]
      }
    ],
    "permissions":[
      "tabs","activeTab"
    ],
    "background":{
      "scripts":["bg.js"]
      
    },
   
    "browser_action": {
      "default_icon":"icon.png",
      "default_title": "musify"
    }
    

  }