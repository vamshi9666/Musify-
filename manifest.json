{
    "manifest_version": 2,
    "name": "musify",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["./lib/p5.min.js","./lib/p5.dom.min.js","./lib/p5.sound.min.js","index.js"]
      }
    ],
    "permissions":[
      "tabs","activeTab","storage"
    ],
    "backround":{
      "page":["popup.html"]
      
    },
   
    "browser_action": {
      "default_popup":"popup.html",
     
      "default_title": "musify"
    }
    

  }